<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | Time Effort</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/fullcalendar.css")" />

    <script src="@Url.Content("~/Scripts/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/moment.min.js")"></script>
    <script src="@Url.Content("~/Scripts/fullcalendar.js")"></script>

    <!-- Bootstrap Core CSS -->
    <link href="@Url.Content("~/Content/AnotherTheme/bower_components/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="@Url.Content("~/Content/AnotherTheme/bower_components/metisMenu/dist/metisMenu.min.css")" rel="stylesheet">

    <!-- Timeline CSS -->
    <link href="@Url.Content("~/Content/AnotherTheme/dist/css/timeline.css")" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="@Url.Content("~/Content/AnotherTheme/dist/css/sb-admin-2.css")" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="@Url.Content("~/Content/AnotherTheme/bower_components/morrisjs/morris.css")" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="@Url.Content("~/Content/AnotherTheme/bower_components/font-awesome/css/font-awesome.min.css")" rel="stylesheet">



    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/Content/AnotherTheme/dist/js/sb-admin-2.js")"></script>

    <style type="text/css">

        .editable input[type=submit] {
            color: #F00;
            font-weight: bold;
        }

        .editable input[type=button] {
            color: #0F0;
            font-weight: bold;
        }
    </style>

</head>
<body>
    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <a class="navbar-brand" href="@Url.Action("Index", "Home")"><img src="@Url.Content("~/Images/cnsHead.png")" style="height:30px" /></a>
        </div>
        <!-- /.navbar-header -->

        @Html.Partial("_LoginPartial")
        <!-- /.navbar-top-links -->

        <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav navbar-collapse">
                <ul class="nav" id="side-menu">
                    <li class="sidebar-search">
                        <div class="input-group custom-search-form">
                            <input type="text" class="form-control" placeholder="Search...">
                            <span class="input-group-btn">
                                <button class="btn btn-default" type="button">
                                    <i class="fa fa-search"></i>
                                </button>
                            </span>
                        </div>
                        <!-- /input-group -->
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Workload")"><i class="fa fa-calendar fa-fw"></i> Workload</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Project")"><i class=" fa fa-file-powerpoint-o fa-fw"></i> Projects</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Monitor")"><i class=" fa fa-pie-chart fa-fw"></i> Monitor</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "User")"><i class=" fa fa-users fa-fw"></i> Users</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Role")"><i class="fa fa-male fa-fw"></i> Roles</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "WorkloadType")"><i class="fa fa-tasks fa-fw"></i> Workload Types</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Position")"><i class="fa fa-signal fa-fw"></i> System roles</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Approve")"><i class=" fa fa-check-circle fa-fw"></i> Approve</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Appoint")"><i class=" fa fa-hand-o-up fa-fw"></i> Appoint</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Customer")"><i class=" fa fa-briefcase fa-fw"></i> Customers</a>
                    </li>
                </ul>
            </div>
            <!-- /.sidebar-collapse -->
        </div>
        <!-- /.navbar-static-side -->
    </nav>
    <div id="page-wrapper">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - LG CNS UZBEKISTAN Time Effort</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
    <!-- jQuery -->
    <script src="@Url.Content("~/Content/AnotherTheme/bower_components/jquery/dist/jquery.min.js")"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="@Url.Content("~/Content/AnotherTheme/bower_components/bootstrap/dist/js/bootstrap.min.js")"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="@Url.Content("~/Content/AnotherTheme/bower_components/metisMenu/dist/metisMenu.min.js")"></script>
    <!-- Morris Charts JavaScript -->
    <script src="@Url.Content("~/Content/AnotherTheme/bower_components/raphael/raphael-min.js")"></script>
    <script src="@Url.Content("~/Content/AnotherTheme/bower_components/morrisjs/morris.min.js")"></script>
    <script src="@Url.Content("~/Content/AnotherTheme/js/morris-data.js")"></script>


    @*<script src="http://www.appelsiini.net/projects/jeditable/jquery.jeditable.js"></script>
    <script src="@Url.Content("~/Scripts/jquery.jeditable.js")" type="text/javascript" charset="utf-8"></script>

    <link rel="stylesheet" type="text/css" href="http://www.prepbootstrap.com/Content/shieldui-lite/dist/css/light/all.min.css" />
    <script type="text/javascript" src="http://www.prepbootstrap.com/Content/shieldui-lite/dist/js/shieldui-lite-all.min.js"></script>
    <script type="text/javascript" src="http://www.prepbootstrap.com/Content/data/shortGridData.js"></script>*@
</body>
</html>
